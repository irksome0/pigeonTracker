{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/utils/db.js", "app/models/User.js", "components/UsersList.tsx", "utils/useSessionStorage.ts", "configs/auth.js", "components/CountrySelector.jsx", "components/PigeonItem.tsx", "components/UsersPigeons.tsx", "components/UserContainer.jsx", "utils/db.js"],
  "exclude": ["node_modules"]
}
